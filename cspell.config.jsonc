  "version": "0.2",
  "import": ["@cspell/cspell-bundled-dicts"],
  "files": ["**"],
  "enableFiletypes": ["*"],
  "enableGlobDot": true,
  "useGitignore": true,
  "ignorePaths": [
    "*.min.js",
    "*.mp3",
    "*.pyc",
    "*.svg",
    ".git/**",
    ".yarn/**",
    "bun.lockb",
    "dist/**",
    "LICENSE",
    "node_modules/**",
    "package-lock.json",
    "pnpm-lock.yaml",
    "yarn.lock",
    // @template-customization-start
    // Assets which are not edited by hand.
    "*.cdr",
    "*.flp",
    "*.pdn",
    // Autogenerated files
    "hanabi.websynths-patch.json",
    "go.sum",
    // Minified dependencies
    "*.min.js",
    "*.min.css",
    // CSS files might have strange paths.
    "hanabi.css",
    // Archived files where spelling errors will not be fixed.
    "/misc/keldon/**",
    // Emoji names are often two words without a space.
    "emojis.json",
    // Most emote names are not words.
    "emotes.json",
    // Word lists
    "word_list.txt",
    "words.txt",
