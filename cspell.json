    "*.min.css",
    // CSS files might have strange paths.
    "hanabi.css",
    // Archived files where spelling errors will not be fixed.
    "/misc/keldon/**",
    // Emoji names are often two words without a space.
    "emojis.json",
    // Most emote names are not words.
    "emotes.json",
